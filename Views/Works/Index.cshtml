            @Html.DisplayFor(modelItem => item.Employee.NAME)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Work_center.WorkcenterName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { employeeID = item.EmployeeID, workcenterID = item.WorkcenterID }) |
