    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.WorkDescription)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Employee.NAME)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Work_center.WorkcenterName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
